            "id": "webhook-dados",
            "type": "webhook",
            "content": {
              "method": "POST",
              "url": "https://api.kryonix.com.br/typebot/leads",
              "headers": [
                {
                  "key": "Authorization",
                  "value": "Bearer 6f78dbffc4acd9a32b926a38892a23f0"
                },
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
