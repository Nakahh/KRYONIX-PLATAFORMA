{
  "name": "kryonix-plataforma",
  "version": "1.0.0",
  "description": "Plataforma SaaS KRYONIX - 100% Autônoma por IA",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "node server.js",
    "production": "NODE_ENV=production node server.js",
    "webhook": "node webhook-listener.js",
    "monitor": "node kryonix-monitor.js",
    "lint": "next lint",
    "check-deps": "node -e \"console.log('🔍 KRYONIX - Verificando dependências críticas...'); const deps = ['next', 'react', 'react-dom', 'express', 'cors', 'helmet', 'body-parser', 'morgan']; let missing = []; deps.forEach(dep => { try { require(dep); console.log('✅ ' + dep + ': OK'); } catch(e) { console.error('❌ ' + dep + ': FALTANDO'); missing.push(dep); } }); if (missing.length === 0) { console.log('🎉 Todas as dependências críticas instaladas!'); } else { console.error('❌ Dependências faltando: ' + missing.join(', ')); process.exit(1); }\"",
    "postinstall": "npm run check-deps && npm run build",
    "prestart": "npm run check-deps"
  },
  "dependencies": {
    "@tailwindcss/forms": "0.5.10",
    "clsx": "2.1.1",
    "lucide-react": "0.427.0",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "express": "4.18.2",
    "cors": "2.8.5",
    "helmet": "7.1.0",
    "body-parser": "1.20.2",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "pg": "8.11.3",
    "bcryptjs": "2.4.3",
    "jsonwebtoken": "9.0.2",
    "ioredis": "5.3.2",
    "aws-sdk": "2.1467.0",
    "socket.io": "4.7.4",
    "node-cron": "3.0.3",
    "axios": "1.6.2",
    "dotenv": "16.3.1",
    "ws": "8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nakahh/kryonix-plataforma.git"
  },
  "keywords": [
    "saas",
    "kryonix",
    "ai",
    "automation",
    "platform",
    "whatsapp",
    "mobile-first"
  ],
  "author": "KRYONIX Team",
  "license": "MIT"
}
